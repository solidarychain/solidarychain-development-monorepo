{
  "name": "@solidary-chain/server-graphql",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "start": "npx env-cmd -f .env ts-node src/main",
    "prestart:prod": "npm run build",
    "start:dev": "npx env-cmd -f .env tsc-watch -p tsconfig.build.json --onSuccess \"node dist/main.js\"",
    "start:debug": "npx env-cmd -f .env tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect=9230 dist/main.js\"",
    "start:dev:network": "npx env-cmd -f network.env tsc-watch -p tsconfig.build.json --onSuccess \"node dist/main.js\"",
    "start:debug:network": "npx env-cmd -f network.env tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect=9230 dist/main.js\"",
    "start:prod": "npx env-cmd -f .env node dist/main.js",
    "start:container": "npx node dist/main.js"
  },
  "dependencies": {
    "@nest-middlewares/cookie-parser": "^6.0.0",
    "@nestjs/common": "6.11.8",
    "@nestjs/core": "6.11.8",
    "@nestjs/graphql": "6.6.1",
    "@nestjs/jwt": "6.1.2",
    "@nestjs/passport": "6.2.0",
    "@nestjs/platform-express": "6.11.8",
    "@solidary-chain/asset-cc": "^0.1.0",
    "@solidary-chain/cause-cc": "^0.1.0",
    "@solidary-chain/common-cc": "^0.1.0",
    "@solidary-chain/participant-cc": "^0.1.0",
    "@solidary-chain/person-cc": "^0.1.0",
    "@solidary-chain/transaction-cc": "^0.1.0",
    "@worldsibu/convector-adapter-fabric": "^1.3.8",
    "apollo-server-express": "2.8.1",
    "bcrypt": "^3.0.6",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.10.0",
    "generate-password": "^1.4.2",
    "graphql": "^14.6.0",
    "graphql-subscriptions": "1.1.0",
    "graphql-type-json": "^0.3.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^0.17.6",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/node": "^12.7.8",
    "@types/passport-jwt": "^3.0.2",
    "@types/passport-local": "^1.0.33",
    "env-cmd": "^10.0.1",
    "ts-node": "^8.8.1",
    "tsc-watch": "^4.0.0",
    "tslint": "5.18.0",
    "typescript": "^3.8.3",
    "uuid": "^3.3.3"
  }
